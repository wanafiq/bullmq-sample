{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es2019",
		"lib": ["es2019"],
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"isolatedModules": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"allowSyntheticDefaultImports": true
	},
	"exclude": ["node_modules"]
}
